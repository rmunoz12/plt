@setup {

group A(Object) {
    func group A __init__() { return this; }
};

group B(A) {
    func str __repr__() {
        return "Hello from class B!";
    }
};

group C(B) {

};

group D(A) {

};

group A no_repr;
group B yes_repr;
group C inherited_repr;
group D inherited_repr_built_in;

}

@turns {

func void begin() {

    print(1); print("\n");
    print("abc"); print("\n");
    print(True); print(" "); print(False); print("\n");
    print(no_repr); print("\n");
    print(no_repr.__repr__()); print("\n");
    print(yes_repr); print("\n");
    print(inherited_repr); print("\n");
    print(inherited_repr_built_in); print("\n");

    end;
}

}
