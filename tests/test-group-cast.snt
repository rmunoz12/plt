@setup {

group A(Object) {
    int x;
    int y;
    func group A __init__(int x, int y) {
        this.x = x; this.y = y;
        return this;
    }
};

group B(A) {
    int z;
    func group B __init__(int x, int y, int z) {
        this.x = x; this.y = y; this.z = z;
        return this;
    }
};

}

@turns {

func void begin() {
    group A a = B(1, 2, 3);

    print(a.y); print("\n");
    end;
}

}
