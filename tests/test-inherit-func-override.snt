@setup {

group A() {
    int x;

    func void __init__(int x) {
        this.x = x;
    }

    func int test() {
        return this.x + 5;
    }

};


group B(A) {
    int y;

    func int test() {
        this.y = 10;
        return this.y;
    }
};

group C(A) {

};

group D(B) {
    func void __init__(int x, int y) {
        this.x = x;
        this.y = y;
    }
};

group A obj;
group B obj2;
group C obj3;
group D obj4;

}

@turns {

func void begin() {
    int y;

    obj.__init__(1);
    obj2.__init__(2);
    obj3.__init__(3);
    obj4.__init__(20, 30);

    print(obj.x); print("\n");
    print(obj2.x); print("\n");
    print(obj3.x); print("\n");
    print(obj4.x); print("\n");
    print(obj4.y); print("\n");

    print(obj.test()); print("\n");
    print(obj2.test()); print("\n");
    print(obj3.test()); print("\n");
    print(obj4.test()); print("\n");

    print(obj4.y); print("\n");

    end;
}

}

