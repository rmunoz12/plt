@setup { int x = 1; int y = 2; int z = 3; }

@turns {

func void begin() {
    int x = 1; int y = 2; int z = 3;
    str s = "hello world!";

    print([]); print("\n");
    print([x, y, z]); print("\n");
    print([3, 4, 5]); print("\n");
    print(["abc", "def"]); print("\n");
    print([s, "123"]); print("\n");
    print([True, False, True]); print("\n");

    end;
}

}
