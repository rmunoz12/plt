@setup {

group A() {
    int x;
    int y;

    func group A __init__(int x, int y) {
        this.x = x;
        this.y = y;
        return this;
    }
};

group B(A(2, 3)) {

};

group A a;
group A aa;
group B b;
group B bb;

}

@turns {

func void begin() {
    print(2 != 3); print("\n");
    print(2 != 2); print("\n");
    print("abc" != "abc"); print("\n");
    print("abcd" != "abc"); print("\n");

    a.__init__(100, 200);
    aa.__init__(200, 300);
    b.__init__();
    bb.__init__();

    print(a != aa); print("\n");
    print(b != b); print("\n");
    print(b != bb); print("\n");
    end;
}

}
